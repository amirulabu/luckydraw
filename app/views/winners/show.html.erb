<div class="page-header">
  <%= link_to winners_path, class: 'btn btn-default' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Back
  <% end %>
  <%= link_to edit_winner_path(@winner), class: 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    Edit
  <% end %>
  <h1>Show winner</h1>
</div>

<dl class="dl-horizontal">
  <dt>Name:</dt>
  <dd><%= @winner.name %></dd>

  <dt>Petid:</dt>
  <dd><%= @winner.petid %></dd>

</dl>
